/*样式重置*/

view, input, button, text, textarea {
  font-family: MicroSoft Yahei;
  font-size: 30rpx;
  color: #2a2a2a;
}

button {
  background: transparent;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0) !important;
  padding: 0;
  margin-left: 0;
  margin-right: 0;
  overflow: visible;
}

button::after {
  border: none;
}

.btn-hover {
  background: transparent;
}

/*基本样式  */

.clear:before, .clear:after {
  display: table;
  content: "";
}

.clear:after {
  clear: both;
}

image {
  display: block;
}

.fl {
  float: left;
}

.fr {
