}

.font36 {
  font-size: 36rpx !important;
}

.font40 {
  font-size: 40rpx !important;
}

.font50 {
  font-size: 50rpx !important;
}

.height25 {
  width: 100%;
  height: 25rpx;
  background: #eff2f9;
}

.height50 {
  height: 50rpx;
}

/*位置样式  */

.row-center {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.row-center-a {
  display: flex;
  flex-direction: row;
  justify-content: center;
   align-items: center; /*垂直居中*/
}

.row-start {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center; /*垂直居中*/
}

.row-start-a {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center; /*垂直居中*/
}

.row-around {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.row-between {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center; /*垂直居中*/
}

.row-between-a {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.column-center {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.column-center-a {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.column-center-b {
  display: flex;
  flex-direction: column;
}

.column-around {
  display: flex;
  flex-direction: column;
}

