<template name="prize-popup2">
  <view wx:if='{{newState2}}' class='popup-bg'>
    <view class='prize-popup2 column-center'>
      <text class='prize-popup2-text'>获得￥{{gift}}元</text>
      <view class='column-center prize-popup2-center' wx:if='{{tradeState}}' style='margin-bottom:150rpx'>
        <text>红包已进入您的水滴币账户</text>
        <text>满300币即可提现0.3元</text>
      </view>
      <view class='column-center prize-popup2-center' wx:if='{{!tradeState}}'>
        <view class='text1'>帮我
          <image src='https://tp.datikeji.com/a/15353717649693/GMnsAdVvjtWkDEiAzBJknHcvQA3LTzeEGUqPcw1W.png' class='prize-popup2-img'></image>后</view>
        <view class='text2'>再送你一个红包</view>
      </view>
      <form bindsubmit="closeTap" report-submit='true' data-type='5' wx:if='{{tradeState}}'>
        <button hover-class="btn-hover" form-type="submit" class='btn6 bg' style='height:94rpx;line-height:94rpx;font-size:40rpx;'>
          立即查看
        </button>
      </form>
      <form bindsubmit="closeTap" report-submit='true' data-type='5' wx:if='{{!tradeState}}'>
        <button hover-class="btn-hover" form-type="submit" class='btn6 bg' style='height:94rpx;line-height:94rpx;font-size:40rpx;'>
          好的
        </button>
      </form>

      <form bindsubmit="closeTap" report-submit='true' data-type='5'>
        <button hover-class="btn-hover" form-type="submit" class='prize-popup-close'>
        </button>
      </form>

    </view>
  </view>
</template>