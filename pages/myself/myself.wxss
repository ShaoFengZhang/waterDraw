@import "../template/bottom/bottomItem.wxss";
page {
  background: #eff2f9;
}
.container{
  position: relative
}
.guide-img{
  width:290rpx;
  height:121rpx;
  position: absolute;
  right:0;
}
.topCon{
  background:#fff;
  padding:60rpx 0 0;
  margin-bottom:25rpx;
}
.myTop {
  margin-bottom: 40rpx;
}
.myTop-head{
  position: relative
}

.head-img {
  width: 112rpx;
  height: 67rpx;
  position: absolute;
  top:-52rpx
}

.myHeader {
  width: 94rpx;
  height: 94rpx;
  border-radius: 100%;
  overflow: hidden;
}

.myName{
  font-size:36rpx;
  margin-top:10rpx;
  height:50rpx;
  line-height:50rpx;
  color:#818181
}

.link-row {
  width: 650rpx;
  margin-bottom: 35rpx;
}

.link-item {
  width: 242rpx;
  height: 106rpx;
  border-radius: 10rpx;
  border:4rpx solid #f95959;
  line-height:48rpx
}

.link-item text {
  color: #f95959;
}

.record-box {
  width: 100%;
  padding: 20rpx 0;
}

.record-list {
  width: 50%;
  height: 160rpx;
}

.record-list:first-child {
  border-right: 1px solid #aeaeae;
}

.icon-img {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 30rpx;
}

.main-list {
  width: 100%;
  background: #fff;
}

.main-item {
  padding: 40rpx 50rpx;
  height: 50rpx;
  line-height:50rpx;
  position: relative
}
.main-item .contact{
  position: absolute;
  width:100%;
  height: 100%;
  left:0;
  top:0
}

.right-point{
  margin-left:15rpx;
  margin-top: -4rpx
}
