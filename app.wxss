/*头部  */

.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
  height: 44px;
  line-height: 44px;
}

.header .title {
  font-size: 16px;
  color: #2d2d2d;
  width: 100%;
  text-align: center;
  display: block;
  letter-spacing: 1px;
  font-weight: bold
}

.navBack {
  width: 10px;
  height: 18px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 13px 30px 13px 20px;
}

.toIndex {
  width: 60px;
  height: 44px;
  position: absolute;
  bottom: 0;
  left: 0;
  background:url('https://tp.datikeji.com/a/15338825205331/3eywXHysimF5F7ySEtYmyY9e4CHzOzjJw6MKMnrx.png') no-repeat;
  background-size: 28px 28px;
  background-position:center center
}
.toIndex image{
  width: 28px;
  height: 28px;
}


/* 点击色 */
.active-hover{
